{
  "name": "IRIS",
  "version": "1.0.0",
  "description": "Grapheel's IRIS webapp",
  "main": "src/client/index.jsx",
  "author": "Tim Lingard",
  "license": "MIT",
  "scripts": {
    "start-dev": "gulp start-dev",
    "start": "node src/server",
    "test": "gulp lint && jest",
    "precommit": "gulp lint && jest"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.8",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "del": "^2.2.2",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sass": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^19.0.2",
    "mongodb-runner": "^3.4.0",
    "react-addons-test-utils": "^15.4.2",
    "webpack": "^1.13.3",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "grapheel-iris-client-api": "^1.0.1",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-select": "^1.0.0-rc.10",
    "react-svg-loader": "^1.1.1",
    "redux": "^3.6.0"
  },
  "babel": {
    "presets": [
      "latest",
      "react"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "import"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "no-plusplus": 0,
      "no-console": 0,
      "react/forbid-prop-types": [
        0
      ],
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List"
          ]
        }
      ]
    }
  }
}
