{
  "name": "IRIS",
  "version": "1.0.0",
  "description": "Grapheel's IRIS webapp",
  "main": "index.js",
  "author": "Tim Lingard",
  "license": "MIT",
  "scripts": {
    "start": "gulp",
    "test": "eslint src && jest",
    "runserver": "echo -e \"Starting Mongodb\"; mongodb-runner start && echo \" \" && nodemon src/server/index.js; echo -e \"Stopping mongodb\"; mongodb-runner stop",
    "precommit": "yarn test",
    "prepush": "yarn test"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.8",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "del": "^2.2.2",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "mongodb-runner": "^3.4.0",
    "react-addons-test-utils": "^15.4.2",
    "webpack": "^1.13.3",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "bcrypt-nodejs": "^0.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "enzyme": "^2.8.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "history": "^4.4.1",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "prop-types": "^15.5.8",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "airbnb"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "import"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "no-plusplus": 0,
      "no-console": 0,
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "only-multiline"
        }
      ],
      "react/forbid-prop-types": [
        0
      ],
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List"
          ]
        }
      ]
    }
  }
}
